{
    "name": "scheb/tombstone-analyzer",
    "description": "Generates reports based on the logs from scheb/tombstone",
    "keywords": ["tombstone", "dead code removal", "static code analysis", "dynamic code analysis", "report"],
    "homepage": "https://github.com/scheb/tombstone-analyzer",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Scheb",
            "email": "me@christianscheb.de"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "scheb/tombstone": "^0.19.0",
        "symfony/config": "^3.3|^4.0",
        "symfony/console": "^2.7|^3.0|^4.0",
        "symfony/yaml": "^3.4|^4.0",
        "nikic/php-parser": "^4.0",
        "sebastian/finder-facade": "^1.2",
        "phpunit/php-text-template": "^1.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "escapestudios/symfony2-coding-standard": "^3.9",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "bin": [
        "tombstone-analyzer"
    ],
    "autoload": {
        "psr-4": {
            "Scheb\\Tombstone\\Analyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scheb\\Tombstone\\Analyzer\\Test\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
